SEQ(LOOP(SEQ(SEQ(ASGN(VAR(counter),CONST(0)),ASGN(VAR(total),CONST(1))),ASGN(VAR(data),FUN(get_values))),IF(AND(LT(VAR(counter),CONST(10)),LT(VAR(total),CONST(1000))),SEQ(SEQ(SEQ(SEQ(SEQ(ASGN(VAR(current),VAR(data)),IF(EQ(VAR(current),CONST(0)),SEQ(ASGN(VAR(counter),PLUS(VAR(counter),CONST(1))),CONTINUE()),SKIP())),ASGN(VAR(total),MUL(VAR(total),VAR(current)))),ASGN(VAR(counter),PLUS(VAR(counter),CONST(1)))),IF(GT(VAR(total),CONST(500)),ASGN(VAR(flag),CONST(1)),ASGN(VAR(flag),CONST(0)))),CONTINUE()),BREAK())),FUN(finalize,VAR(total),VAR(flag)))
